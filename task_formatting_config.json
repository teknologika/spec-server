{
  "auto_format_enabled": false,
  "auto_requirements_linking": true,
  "content_redistribution_enabled": true,
  "llm_validation_enabled": true,
  "llm_validation_confidence_threshold": 0.7,
  "allow_manual_override": true,
  "max_validation_retries": 3,
  "classification_confidence_threshold": 0.5,
  "keyword_matching_enabled": true,
  "phrase_matching_enabled": true,
  "requirements_relevance_threshold": 0.2,
  "max_requirements_per_task": 3,
  "auto_add_tbd_placeholders": true,
  "enable_caching": true,
  "cache_ttl_seconds": 300,
  "batch_processing_enabled": true,
  "max_batch_size": 50,
  "fail_on_formatting_errors": false,
  "log_formatting_changes": true,
  "log_level": "INFO",
  "fallback_to_original_on_error": true,
  "preserve_original_backup": true,
  "enable_metrics": true,
  "metrics_collection_interval": 60
}
